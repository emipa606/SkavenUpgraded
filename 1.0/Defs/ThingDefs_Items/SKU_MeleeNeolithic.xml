<Defs>
  <ThingDef ParentName="BaseMeleeWeapon_Sharp_Quality">
    <defName>War_Spear</defName>
    <label>War Spear</label>
    <description>A short and nasty looking weapon of ancient design. Good for stabbing or slashing. It's light and nimble, which sometimes lets it beat bigger, heavier weapons.</description>
    <graphicData>
      <texPath>Things/Weapons/Melee/War_Spear</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <techLevel>Neolithic</techLevel>
    <costStuffCount>50</costStuffCount>
    <weaponTags>
      <li>MedievalMeleeBasic</li>
      <li>NeolithicMeleeBasic</li>
    </weaponTags>
    <statBases>
      <WorkToMake>8000</WorkToMake>
      <Mass>0.85</Mass>
    </statBases>
    <equippedStatOffsets>
      <MeleeHitChance>0.10</MeleeHitChance>
    </equippedStatOffsets>
    <equippedAngleOffset>-65</equippedAngleOffset>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <tools>
      <li>
        <label>point</label>
        <capacities>
          <li>Stab</li>
        </capacities>
        <power>12</power>
        <cooldownTime>1.4</cooldownTime>
        <armorPenetration>0.50</armorPenetration>
      </li>
      <li>
        <label>edge</label>
        <capacities>
          <li>Cut</li>
        </capacities>
        <power>11</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
    <recipeMaker>
      <skillRequirements>
        <Crafting>3</Crafting>
      </skillRequirements>
    </recipeMaker>
  </ThingDef>
